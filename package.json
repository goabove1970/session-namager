{
  "name": "dinero-session-manager",
  "version": "3.2.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www",
    "start-local": "NODE_ENV=development npm run start",
    "build": "npm run compile",
    "build:ts": "tsc",
    "compile": "npm run build:ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPatterns=integration",
    "prettier:check": "prettier --check \"./**/*.{js,ts,tsx,json,scss,md}\"",
    "prettier:write": "prettier --write \"./**/*.{js,ts,tsx,json,scss,md}\"",
    "pack": "node ./pack.js",
    "patch": "./scripts/bump-patch.sh",
    "minor": "./scripts/bump-minor.sh",
    "major": "./scripts/bump-major.sh",
    "up": "node ./upload.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "guid-typescript": "^1.0.9",
    "http-errors": "^2.0.1",
    "module-alias": "^2.2.3",
    "moment": "^2.29.4",
    "winston": "^3.19.0",
    "zip-a-folder": "^1.1.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.17.0",
    "@types/supertest": "^6.0.3",
    "grunt": "^1.5.3",
    "grunt-bump": "^0.8.0",
    "jest": "^30.0.4",
    "pack-zip": "^0.2.2",
    "prettier": "^3.7.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.2.0",
    "ts-postgres": "^1.1.0",
    "typescript": "^5.9.3"
  },
  "_moduleAliases": {
    "@root": "dist",
    "@src": "dist/src",
    "@models": "dist/src/models",
    "@routes": "dist/src/routes",
    "@utils": "dist/src/utils",
    "@controllers": "dist/src/controllers"
  }
}
